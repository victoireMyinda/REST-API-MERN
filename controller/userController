const asyncHandler = require("express-async-handler")

const createUser = asyncHandler(async(request, response) => {
    response.status(200).json({ message: "GET success" })
})

const findUser = asyncHandler(async(request, response) => {
    response.status(200).json({ message: "SET success" })
})

const updateUser = asyncHandler(async(request, response) => {
    response.status(200).json({ message: "UPDATE success" })
})

const deleteUser = asyncHandler(async(request, response) => {
    response.status(200).json({ message: "DELETE success" })
})


module.exports = {
    createUser,
    findUser,
    updateUser,
    deleteUser
}